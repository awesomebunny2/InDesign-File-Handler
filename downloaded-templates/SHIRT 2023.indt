var version = detectIE();
if (version === false) {
  document.documentElement.className+='ie-false';
} else if (version >= 12) {
  document.documentElement.className+='ie-false edge edge' + version;
} else {
  document.documentElement.className+='ie ie' + version;
}
// add details to debug result
